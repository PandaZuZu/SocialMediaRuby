<class id="featured_users_wrapper">

    <% if @featured_users.any? %>
        <% @featured_users.each do |user| %>
            <%= user.first_name %>
            <br/>
        <% end %>
    <% else %>
        nothing
    <% end %>
    
</class>

<class id= "login_register_wrapper">

    <%= link_to('Login', users_path) %>
    
</class>

