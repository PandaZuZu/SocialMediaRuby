
<ul>
    <% if @current_user != nil %>
        <%= @current_user.first_name %>
    
        <% if @current_user.last_name != nil %>
            <%= @current_user.last_name %>
        <% else %>
            empty
        <% end %>
    <br/>
    
        <% if @current_user.interests.any? %>
            <% @current_user.interests.each do |i| %>
                <%= i.id %>
            <% end %>
        <% end %>
    
    <% else %>
        nNULL
    <% end %>
    
</ul>